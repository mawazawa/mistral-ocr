MISTRAL_API_KEY=
MISTRAL_OCR_MODEL=mistral-ocr-latest
MISTRAL_QA_MODEL=mistral-small-latest
# When using `vercel dev`, API routes run on port 3000 by default
VITE_API_BASE_URL=http://localhost:3000

# Upload limits (defaults used if unset)
# Server-side max decoded bytes accepted by /api/ocr (default ~4.5MB)
MAX_UPLOAD_BYTES=4500000
# Client-side guard to prevent selecting huge files (default ~4.5MB)
VITE_MAX_UPLOAD_BYTES=4500000
